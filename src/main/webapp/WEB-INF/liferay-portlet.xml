<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
    <portlet>
        <portlet-name>Cluster Monitor</portlet-name>
        <icon>/img/icon.png</icon>
        <control-panel-entry-category>server</control-panel-entry-category>
        <control-panel-entry-weight>5</control-panel-entry-weight>
        <control-panel-entry-class>au.com.permeance.clusterMonitor.controller.ControlPanelEntryImpl</control-panel-entry-class>
        <instanceable>false</instanceable>
        <remoteable>false</remoteable>
        <ajaxable>false</ajaxable>
        <header-portlet-css>/css/cluster-monitor.css</header-portlet-css>
        <footer-portlet-javascript>/js/cluster-monitor.js</footer-portlet-javascript>
        <system>true</system>
    </portlet>
    <role-mapper>
        <role-name>administrator</role-name>
        <role-link>Administrator</role-link>
    </role-mapper>
</liferay-portlet-app>
